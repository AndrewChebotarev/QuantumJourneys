<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="QuantumJourneys.Pages.NewOrOldGameSolo"
             Title="NewOrOldGameSolo">
    <VerticalStackLayout>
        <Label 
            Text="В разработке!"
            VerticalOptions="Center" 
            HorizontalOptions="Center" />
        <Button
                
                x:Name="SoloGameBtn"
                Clicked="SoloGameBtn_Clicked"
                Text="Назад"
                
                HorizontalOptions="Center" 
                VerticalOptions="End"
                Margin="0,40,0,40"
                
                BackgroundColor="#5C2ACE"
                FontSize="Title"
                TextColor="#FFFFFF"
                BorderWidth="1"
                BorderColor="#000000"/>
        
    </VerticalStackLayout>
</ContentPage>